{
    "name": "roave/signature",
    "description": "Sign and verify stuff",
    "license": "MIT",
    "require": {
        "php": "^7.1 || ^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "infection/infection": "^0.7.1",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Roave\\Signature\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roave\\SignatureTest\\": "test/unit",
            "Roave\\SignatureTestFixture\\": "test/fixture"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
