{
    "name": "roave/signature",
    "description": "Sign and verify stuff",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "infection/infection": "^0.17.5",
        "doctrine/coding-standard": "^8.1",
        "vimeo/psalm": "^3.16"
    },
    "autoload": {
        "psr-4": {
            "Roave\\Signature\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roave\\SignatureTest\\": "test/unit/src",
            "Roave\\SignatureTestFixture\\": "test/fixture"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
