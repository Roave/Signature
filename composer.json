{
    "name": "roave/signature",
    "description": "Sign and verify stuff",
    "license": "MIT",
    "require": {
        "php": "7.4.*|8.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.1",
        "infection/infection": "^0.20.2",
        "doctrine/coding-standard": "^8.2",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Roave\\Signature\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roave\\SignatureTest\\": "test/unit/src",
            "Roave\\SignatureTestFixture\\": "test/fixture"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
